/* 公共按钮 */
.user-set-btn{
	width: 100%;
	margin: 20rpx 0;
	border: 0!important;
}
.user-set-btn-disable{
	background: #F4F4F4!important;
	border: 1rpx solid #EEEEEE!important;
	color: #909090!important;
}
.body{
	width: 100%;
	padding: 0 20rpx!important;
}
.common-input{
	font-size: 30rpx;
	border-bottom: 1rpx solid #F4F4F4;
}
.text-right {
	text-align: right;
}
.user-set-userinfo-list {
	padding: 20rpx;
	border-bottom: 1rpx solid #F4F4F4;
}
.user-set-userinfo-list>view:first-child {
	font-size: 32rpx;
	font-weight: bold;
	color: #9B9B9B;
}
.user-set-userinfo-list>view:last-child>image {
	width: 80rpx;
	height: 80rpx;
	border-radius: 100%;
}
.user-set-userinfo-list .icon-fabu {
	margin-left: 20rpx;
	color: #9B9B9B;
}

